{
  "manifest_version": 2,
  "name": "mooinPLUS",
  "description": "Makes working with mooin a little more comfortable.",
  "version": "0.1",
  "icons": {"128": "icons/active128.png"},

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
      "default_title": "mooinPLUS",
      "default_popup": "popup.html"
  },
  "permissions": [
      "activeTab",
      "https://mooin.oncampus.de/*"
  ]
}
